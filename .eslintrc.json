{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "prettier"
      ],
    "rules": {
        "prettier/prettier": ["error", {
           "endOfLine":"auto"
         }],
        "camelcase": "off",
        "no-param-reassign": "off"
    }
}
